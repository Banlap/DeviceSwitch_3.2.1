    <style name="custom_seekbar">
        <item name="android:maxHeight">2dp</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/ipc_seekbar_line</item>
        <item name="android:minHeight">1dp</item>
        <item name="android:paddingStart">0dp</item>
        <item name="android:paddingEnd">0dp</item>
        <item name="android:thumb">@drawable/ipc_seekbar_thumb</item>
    </style>
    <style name="myTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#3F51B5</item>
        <item name="colorPrimaryDark">#303F9F</item>
        <item name="colorAccent">#FF4081</item>
    </style>
    <style name="video_repaly">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/ipc_hint_replay_bg</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:orientation">vertical</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingTop">14dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:paddingBottom">14dp</item>
    </style>
    <style name="window_background" parent="android:Theme.Holo.Light.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>

    </style>
    <declare-styleable name="TimeRulerView">
        <!--刻度颜色-->
        <attr format="reference|color" name="ipc_gradationColor"/>
        <!--刻度宽度-->
        <attr format="dimension|reference" name="ipc_gradationWidth"/>
        <!--刻度数值颜色-->
        <attr format="color|reference" name="ipc_gradationTextColor"/>
        <!--刻度数值大小-->
        <attr format="dimension|reference" name="ipc_gradationTextSize"/>
        <!--秒刻度的长度-->
        <attr format="dimension|reference" name="ipc_secondLen"/>
        <!--分刻度的长度-->
        <attr format="dimension|reference" name="ipc_minuteLen"/>
        <!--时刻度的长度-->
        <attr format="dimension|reference" name="ipc_hourLen"/>
        <!--当前时间，单位：s-->
        <attr format="integer|reference" name="ipc_currentTime"/>
        <!--中间指针线颜色-->
        <attr format="reference|color" name="ipc_indicatorLineColor"/>
        <!--中间指针线宽度-->
        <attr format="reference|dimension" name="ipc_indicatorLineWidth"/>
        <!--时间块的颜色-->
        <attr format="color|reference" name="ipc_partColor"/>
    </declare-styleable>
